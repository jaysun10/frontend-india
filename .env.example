# Frontend Environment Variables Example
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api

# Contact Information
VITE_WHATSAPP_NUMBER=+1234567890
VITE_TELEGRAM_USERNAME=@escort_service
VITE_PHONE_NUMBER=+1234567890
VITE_EMAIL=contact@escortservice.com

# External Services
# VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# VITE_FACEBOOK_PIXEL_ID=your_facebook_pixel_id

# Payment Configuration
# VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Map Services
# VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Social Media Links
VITE_FACEBOOK_URL=https://facebook.com/escortservice
VITE_INSTAGRAM_URL=https://instagram.com/escortservice
VITE_TWITTER_URL=https://twitter.com/escortservice

# SEO Configuration
VITE_SITE_NAME=The Escort Service
VITE_SITE_DESCRIPTION=Premium companion services with discretion and elegance
VITE_SITE_URL=https://yourdomain.com

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_CHAT_WIDGET=false
VITE_ENABLE_BOOKING_SYSTEM=true

# Development Configuration
VITE_DEBUG_MODE=true
VITE_SHOW_CONSOLE_LOGS=true